package toDoList;

import java.util.function.Consumer;

public interface TarefasInterface {
	static Consumer<String> print = str -> System.out.print(str);



	abstract void addTarefa();

	abstract void listarTarefas();


	abstract void marcarTarefas(int id);

	abstract void excluirTarefasConcluidas();

	abstract void salvarTarefas();

}
