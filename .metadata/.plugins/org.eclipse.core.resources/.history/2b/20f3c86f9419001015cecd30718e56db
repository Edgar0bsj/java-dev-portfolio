package toDoList;

public class InitSystem {
	public static void main(String[] args) {
		GerenciamentoTarefas toDos = new GerenciamentoTarefas();
		
		
		toDos.addTarefa();
		toDos.addTarefa();

		toDos.listarTarefas();
		
		toDos.excluirTarefasConcluidas();
		toDos.marcarTarefas(1);
		
		toDos.listarTarefas();
	}

}
